(* Basic weak secret example, from JLAP paper
   First version, with an attack *)

fun pk/1.
fun penc/2.
fun pdec/2.
equation pdec(penc(x, pk(y)), y) = x.

fun h/1.

free c.
private free w.

weaksecret w.

process
	new s;
	out(c, pk(s));
	out(c, penc(h(w), pk(s)))

(* EXPECTPV
RESULT Weak secret w is false.
