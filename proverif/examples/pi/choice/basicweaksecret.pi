(* Basic weak secret example, from JLAP paper
   Corrected version *)

fun pk/1.
fun penc/3.
fun pdec/2.
equation pdec(penc(x, pk(y), z), y) = x.

fun h/1.

free c.
private free w.

weaksecret w.

process
	new s;
	new a;
	out(c, pk(s));
	out(c, penc(h(w), pk(s), a))

(* EXPECTPV
RESULT Weak secret w is true.
0.008s (user 0.008s + system 0.000s), max rss 8568K
END *)
